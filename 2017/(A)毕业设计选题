#include <iostream>
#include <string>

using namespace std;

int main(){
    int num=0;
    int line=0;

    cin>>num;
    for(int i=0;i<num;++i){
        cin>>line;
        string s;
        double gds=0;
        double work=0;

        string str;
        double quo=0;
        for(int j=0;j<line;++j){
            cin>>s>>gds>>work;
            if((gds/work)>quo){
                quo=gds/work;
                str=s;
            }
        }
        cout<<str<<endl;
    }

    return 0;
}
