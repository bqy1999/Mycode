	int c = 0;//是int类型，不是char类型
	while ((c = getchar()) != EOF)//EOF是一个宏定义的实数，一般为-1，但是不同条件下的值不同，所以一般不推荐用-1
		putchar(c);
	EOF是表示的字符流的结束，虽然它的名字是文件结束
	当输入ctrl+z时，就表示在控制台结束

	格式化输出
		int printf(const char* format, ...);
		format:%[flage][width][.precision][length]specifier
		常用的输出格式
		整型 %d
		浮点型
		printf("Character:%c %c \n", 'a', 65);

	格式化输入
		int scanf(const char *format, ...);
		输入的返回值(较重要)
		Failure的话返回EOF
		Success的话返回成功输入的变量的个数

	char str[80];
	int i = 0;
	printf("Enter your family name:");
	scanf("%79s", str);//长为79的字符串，将输入的字符串赋值给str
	printf("Enter your age:");
	scanf("%d", &i);
	printf("Mr. %s ,%d years old", str, i);



HuNan University ACM begining
A+B for Input-Output Practice
Please refer to https://vjudge.net/contest/183328 if you need it.

1-1(A)
#include<stdio.h>

int main()
{
	int a = 0;
	int b = 0;
	int c = 0;

	while (scanf("%d %d", &a, &b) != EOF)
	{
		printf("%d\n", a + b);
	}

	return 0;
}
//

1-2(B)
#include<stdio.h>

int main()
{
	int num = 0;
	int a = 0;
	int b = 0;

	scanf("%d", &num);

	for (int i = 0; i < num; i++)
	{
		scanf("%d %d", &a, &b);
		printf("%d\n", a + b);
	}

	return 0;
}
//

1-3(C)
#include<stdio.h>

int main()
{
	int a = 0;
	int b = 0;

	while (scanf("%d %d", &a, &b) != EOF)
	{
		if (a == 0 && b == 0)break;
		printf("%d\n", a + b);
	}

	return 0;
}
//

1-4(D)
#include<stdio.h>

int main()
{
	int num = 0;
	int a = 0;
	int sum = 0;

	while (scanf("%d", &num)==1)
	{
		sum = 0;
		if (num == 0)break;
		for (int i = 0; i < num; i++)
		{
			scanf("%d", &a);
			sum += a;
		}
		printf("%d\n", sum);
	}

	return 0;
}
//

1-5(E)
#include<stdio.h>

int main()
{
	int num = 0;
	int a = 0;
	int sum = 0;
	int b = 0;

	scanf("%d", &b);

	for (int j = 0; j < b; j++)
	{
		scanf("%d", &num);

		sum = 0;
		for (int i = 0; i < num; i++)
		{
			scanf("%d", &a);
			sum += a;
		}
		printf("%d\n", sum);
	}
	
	return 0;
}
//

1-6(F)
#include<stdio.h>

int main()
{
	int num = 0;
	int a = 0;
	int sum = 0;

	while (scanf("%d", &num) != EOF)
	{
		sum = 0;
		for (int i = 0; i < num; i++)
		{
			scanf("%d", &a);
			sum += a;
		}
		printf("%d\n", sum);
	}
	
	return 0;
}
