1-7(G)
#include<stdio.h>

int main()
{
	int a = 0;
	int b = 0;

	while (scanf("%d %d", &a , &b) != EOF)
	{
		printf("%d\n\n", a + b);
	}
	
	return 0;
}
//

1-8(H)
#include<stdio.h>

int main()
{
	int num = 0;
	int a = 0;
	int b = 0;
	int c = 0;

	scanf("%d", &num);

	for (int i = 0; i < num; i++)
	{
		int sum = 0;
		scanf("%d", &a);
		for (int j = 0; j < a; j++)
		{
			scanf("%d", &b);
			sum += b;
		}
		printf("%d\n", sum);
		if(i!=num-1)
		printf("\n");
	}
	scanf("%d", &c);
	
	return 0;
}
