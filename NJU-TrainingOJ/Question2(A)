#include <iostream>
using namespace std;
 
int getnum(int* array,int initpounds,int days){
    for(int i=0;i<10;i++){
        array[i]=initpounds;
    }
 
    for(int j=10;j<=100;j=j+5){
        array[j]=array[j-10]+array[j-5];
        array[j+1]=array[j];
        array[j+2]=array[j];
        array[j+3]=array[j];
        array[j+4]=array[j];
    }
 
    return array[days];
}
 
int main() {
    int initpounds = 0;
    int days = 0;
    int *array = new int[105];
 
    while (cin >> initpounds) {
        cin >> days;
        cout << getnum(array, initpounds, days)<<endl;
    }
 
    return 0;
}
