求数列的和

#include <iostream>
#include <cmath>
#include <cstdio>

using namespace std;

int main(){
    int m=0;
    double n,sum=0;
    while (cin>>n>>m){
        sum=n;
        for (int i = 0; i <m-1 ; ++i) {
            n=(double)sqrt(n);
            sum+=n;
        }
        printf("%.2f\n",sum);
    }
    return 0;
}
