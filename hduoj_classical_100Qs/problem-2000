//ASCII码排序

#include <iostream>
#include <cstdio>

using namespace std;

int main(){
    char a,b,c;
    while(cin>>a>>b>>c){
        if(a<b&&b<c)printf("%c %c %c",a,b,c);
        else if(a<c&&c<b)printf("%c %c %c",a,c,b);
        else if(b<a&&a<c)printf("%c %c %c",b,a,c);
        else if(b<c&&c<a)printf("%c %c %c",b,c,a);
        else if(c<a&&a<b)printf("%c %c %c",c,a,b);
        else if(c<b&&b<a)printf("%c %c %c",c,b,a);
        printf("\n");
    }
    return 0;
}
